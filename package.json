{
  "name": "hmda-platform-ui",
  "version": "0.0.0",
  "description": "Frontend code for the HMDA filing system",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "snyk": "snyk test",
    "clean": "rimraf ./dist && mkdir -p ./dist ./dist/js ./dist/css ./dist/fonts",
    "start": "./server/server.js",
    "dev:js": "browserify ./src/js/index.js > ./dist/js/app.min.js",
    "dev:less": "lessc ./src/less/app.less ./dist/css/app.css && cp node_modules/cf-icons/src/fonts/* ./dist/fonts",
    "dev:html": "cp src/index.html dist/index.html",
    "dev:build": "npm run dev:js && npm run dev:less && npm run dev:html",
    "postinstall": "npm run clean",
    "watch:js": "nodemon -e js,jsx -w src/js -x 'npm run dev:js'",
    "watch:tests": "nodemon -e js -w __tests__ -x 'npm test'",
    "watch:less": "nodemon -e less -w src/less -x 'npm run dev:less'",
    "watch:html": "nodemon -e html -w src -x 'npm run dev:html'",
    "watch:server": "nodemon -e js,json -w server -x 'npm start'",
    "watch": "npm run watch:server & npm run watch:js & npm run watch:tests & npm run watch:less & npm run watch:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfpb/hmda-platform-ui.git"
  },
  "keywords": [
    "HMDA"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cfpb/hmda-platform-ui/issues"
  },
  "homepage": "https://github.com/cfpb/hmda-platform-ui#readme",
  "devDependencies": {
    "babel-jest": "11.0.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "express": "4.13.4",
    "express-history-api-fallback": "2.0.0",
    "jest-cli": "11.0.0",
    "less": "2.6.1",
    "nodemon": "1.9.1",
    "react-addons-test-utils": "15.0.1",
    "reactify": "1.1.1",
    "rimraf": "2.5.2",
    "snyk": "1.13.3"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "dependencies": {
    "cf-buttons": "3.0.4",
    "cf-core": "3.2.2",
    "cf-expandables": "3.0.4",
    "cf-forms": "3.0.4",
    "cf-grid": "3.1.2",
    "cf-tables": "1.0.2",
    "cf-theme-cfpb": "1.0.0",
    "cf-typography": "3.0.5",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-router": "2.2.1",
    "superagent": "1.8.3"
  }
}
